pipeline {
    agent {
        docker {
            image '34.121.77.67:8123/ubuntu:1.0.5'
        }
    }
        
    stages {
        stage('git clone project') {
            steps {
                git 'https://github.com/dsolovew/home10.git'
            }
        }

        stage('build') {
            steps {
                sh 'docker build -f dockerfile -t boxfuse .'
                sh 'docker tag boxfuse 34.121.77.67:8123/boxfuse'
                sh 'docker push 34.121.77.67:8123/boxfuse'
            }
        }
    }    
        
    }
